  "components/epicHeader/mob-header": "  <div class=\"row align-items-center\">\r\n    <div class=\"col-3\">\r\n        <a href=\"#main-content\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n            <span class=\"icon\">\r\n                <svg><use xlink:href=\"#icon-menu-search\" /></svg>\r\n            </span>\r\n        </a>\r\n    </div>\r\n    <div class=\"col-7\" style=\"padding: 0;\">\r\n        {{#if theme_settings.use-mobile-logo}}\r\n            {{> components/common/mobile-logo}}\r\n        {{else}}\r\n            {{> components/common/store-logo}}\r\n        {{/if}}\r\n    </div>\r\n    <div class=\"col-2 text-right mobile-cart-btn\">\r\n        <a\r\n                        class=\"\"\r\n                        data-cart-preview\r\n                        data-dropdown=\"cart-preview-dropdown\"\r\n                        data-options=\"align:right\"\r\n                        href=\"{{urls.cart}}\">\r\n                        <span class=\"navUser-item-cartLabel\">\r\n                            <span class=\"icon\">\r\n                                <svg><use xlink:href=\"#icon-shopping-cart\" /></svg>\r\n                            </span>\r\n                        </span><span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n                    </a>\r\n                    <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n        \r\n    </div>\r\n</div>\r\n",
