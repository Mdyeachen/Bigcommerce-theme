{
  "ysw/components/header/header": "\r\n\r\n<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\r\n{{#if theme_settings.use-yotpo}}\r\n    <input type=\"hidden\" value=\"{{theme_settings.yotpo-app-key}}\" name=\"yotpo_key\" id=\"yotpo-key\">\r\n{{/if}}\r\n<header class=\"site-header clean sticky-header {{#if theme_settings.ysw_header_banner_enable}}has-banner{{/if}}\">\r\n    {{#if theme_settings.ysw_header_banner_enable}}\r\n    <div class=\"ysw-banner\">\r\n        <a href=\"{{theme_settings.ysw_home_banner_link}}\">\r\n            <div class=\"container\">\r\n                <p>{{theme_settings.ysw_header_banner_text}}</p>\r\n            </div>\r\n        </a>\r\n    </div>\r\n    {{/if}}\r\n    \r\n    <section class=\"top-bar navUser\" style=\"display: none;\">\r\n        {{> components/epicHeader/top-bar}}\r\n    </section>\r\n    \r\n    <section class=\"logo-bar\">\r\n        {{> components/epicHeader/logo-bar}}\r\n    </section>\r\n    <section class=\"mob-header\">\r\n        {{> components/epicHeader/mob-header}}\r\n    </section>\r\n    <div>\r\n        {{> ysw/addons/mega-nav/mega-nav-menu}}\r\n    </div>\r\n</header> \r\n{{{region name=\"header_bottom--global\"}}}\r\n{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    {{#each (limit banners.top_metadata 1)}}\r\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\r\n        <div data-event-type=\"promotion-click\">\r\n            {{{this.content}}}\r\n        </div>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n{{/if}}\r\n{{{region name=\"header_bottom\"}}}\r\n\r\n\r\n<style>\r\n    .logo-bar{\r\n\t    padding: 0;\r\n    }\r\n        .site-header.sticky-header {\r\n    \tposition: sticky;\r\n    \ttop: 0;\r\n    \tleft: 0;\r\n    \tz-index: 99;\r\n    \t-webkit-box-shadow: 0px 2px 5px 1px rgba(227,227,227,1);\r\n    \t-moz-box-shadow: 0px 2px 5px 1px rgba(227,227,227,1);\r\n    \tbox-shadow: 0px 0px 5px 1px rgb(227, 227, 227);\r\n    }\r\n    \r\n    @media (min-width: 801px) {\r\n      .ysw-banner {\r\n        \theight: 35px;\r\n        \tpadding: 0;\r\n        }\r\n}\r\n</style>\r\n",
  "ysw/addons/mega-nav/mega-nav-menu": "<div class=\"ysw-c-mega-nav navPages-container\" id=\"menu\">\r\n    <div class=\"ysw-c-mega-nav__wrapper\" style=\"background-color: #acacac;\">\r\n        <div class=\"navPages-quickSearch\">\r\n            {{#if theme_settings.epic_hide_search_navigation}}\r\n                {{> components/common/quick-search data='false'}}\r\n            {{/if}}\r\n\r\n            <div class=\"mobile-call-us\">\r\n                <a href=\"tel:{{settings.phone_number}}\">Call Us</a>\r\n            </div>\r\n            {{#if customer}}\r\n            <div class=\"mobile-my-account\">\r\n                <a href=\"{{urls.account.index}}\">My Account</a>\r\n            </div>\r\n            <div class=\"mobile-my-account\">\r\n                <a href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n            </div>\r\n            {{else}}\r\n                <div class=\"mobile-my-account\">\r\n                    <a href=\"{{urls.auth.login}}\">Sign in / Register</a>\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n        <nav class=\"ysw-c-mega-nav__navigation navPages\">\r\n            {{#if theme_settings.navigation_design '===' 'custom'}}\r\n                {{>\r\n                    ysw/addons/mega-nav/mega-nav-button\r\n                    position=\"left\"\r\n                    icon=\"chevron-down\"\r\n                }}\r\n            {{/if}}\r\n\r\n            <ul class=\"ysw-nav-item-last ysw-c-mega-nav__list navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n                {{#if theme_settings.navigation_design '===' 'custom'}}\r\n                    {{#for 0 (subtract theme_settings.ysw_meganav_item_qty 1)}}\r\n                        {{> ysw/addons/mega-nav/mega-nav-item theme_settings=../theme_settings}}\r\n                    {{/for}}\r\n                {{/if}}\r\n            </ul>\r\n            \r\n            <style>\r\n                \