{
  "components/epicHeader/top-bar": "<style>\r\n    \r\n    .ubaid {\r\n        margin-top: -14px;\r\n        color: white;\r\n        font-weight: 600;\r\n\r\n    }\r\n    .invert_effect {\r\n            filter: invert(100%);\r\n        }\r\n</style>\r\n\r\n\r\n<div class=\"container-fluid\">\r\n    <div class=\"row top-bar-inner\">\r\n        <div class=\"col-md-5\">\r\n           {{#if theme_settings.use-google-translate}}\r\n            <div class=\"top-google-translate\"> \r\n                {{> components/epicContent/google-translate}}\r\n            </div>\r\n            {{/if}}\r\n            <div class=\"top-currency-selector\">\r\n                {{> components/common/currency-selector}}\r\n            </div>\r\n            <div class=\"ubaid\" >\r\n                <img \r\n                    style=\"width: 32px; height: 32px; \r\n    filter: invert(1);\"\r\n                    src=\"https://cdn11.bigcommerce.com/s-yfjn5bpc6d/images/stencil/original/image-manager/return.png\" alt=\"Luxe Modz Body Jewelry Online  Free Returns Policy\" title=\"Luxe Modz Body Jewelry Online  Free Returns Policy\">\r\n                Free Returns on all orders.&nbsp; <a href=\"https://track.shipstation.com/f/r/b/SW0pkJ9U8l10lTf0rc5rlA%3D%3D#\" style=\"font-weight: 800;\">Details</a></div>\r\n        </div>\r\n        <div class=\"col-md-7 text-right padding-right-zero\">\r\n            <ul class=\"navUser-section top-bar-menu\">\r\n                <li>\r\n                    <a class=\"navUser-item--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\r\n                </li>\r\n                {{#if theme_settings.show-help-center}}\r\n                    {{> components/custom/top-bar-additonal-content}}\r\n                {{/if}}\r\n                {{#if customer}}\r\n                <li>\r\n                    <a href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\">\r\n                        {{lang 'common.account'}}\r\n                    </a>\r\n                </li>\r\n                <li>\r\n                    <a href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n                </li>\r\n                {{else}}\r\n                <li>\r\n                   <!-- <a href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a> -->\r\n                    <a href=\"javascript:void(0);\" data-dropdown=\"login-drop\" aria-controls=\"login-drop\" aria-expanded=\"false\">{{lang 'common.login'}} / {{lang 'common.sign_up'}}</a> \r\n                    <div id=\"login-drop\" data-dropdown-content class=\"f-dropdown login-drop-contennt content\" aria-autoclose=\"false\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                        <h4>Returning Customer</h4>\r\n                        <form class=\"login-form form\" action=\"/login.php?action=check_login\" method=\"post\" name=\"FormLogin\">\r\n                            <div class=\"form-field form-field--input form-field--inputEmail\">\r\n                                <label class=\"form-label\" for=\"login_email\">Email Address:</label>\r\n                                <input class=\"form-input\" name=\"login_email\" id=\"login_email\" type=\"email\">\r\n                            </div>\r\n                            <div class=\"form-field form-field--input form-field--inputPassword\">\r\n                                <label class=\"form-label\" for=\"login_pass\">Password:</label>\r\n                                <input class=\"form-input\" id=\"login_pass\" type=\"password\" name=\"login_pass\">\r\n                            </div>\r\n                            <div class=\"form-actions\">\r\n                                <input type=\"submit\" class=\"button button--primary\" value=\"Sign in\">\r\n                                <a class=\"forgot-password\" href=\"/login.php?action=reset_password\">Forgot your password?</a>\r\n                            </div>\r\n                        </form>\r\n                        {{#if settings.account_creation_enabled}}\r\n                        <div class=\"signup-link\">\r\n                            <h4>New Customer</h4>\r\n                            <p>Registering provides you with personalized services, including: wish list, order status and promotions.</p>\r\n                            <a class=\"button\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\r\n                        </div>\r\n                        {{/if}}\r\n                    </div>\r\n                </li>\r\n                <!--<li class=\"signup\">\r\n                    {{#if settings.account_creation_enabled}}\r\n                    <a href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\r\n                    {{/if}}\r\n                </li>-->\r\n                {{/if}}\r\n                {{#if settings.gift_certificates_enabled}}\r\n                    <li>\r\n                        <a href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\r\n                    </li>\r\n                {{/if}}\r\n               <!-- <li class=\"top-bar-cart navUser-item--cart\">\r\n                    <a\r\n                        class=\"\"\r\n                        data-cart-preview\r\n                        data-dropdown=\"cart-preview-dropdown\"\r\n                        data-options=\"align:right\"\r\n                        href=\"{{urls.cart}}\">\r\n                        <span class=\"navUser-item-cartLabel\"><i class=\"far fa-shopping-cart\" aria-hidden=\"true\"></i></span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n                    </a>\r\n                    <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n                </li> -->\r\n                <li>\r\n                    <a href=\"javascript:void(0)\" id=\"trigger-overlay\" class=\"dropdown-button\">Shop Info</a>\r\n                </li>\r\n            </ul>\r\n        </div>\r\n    </div>\r\n</div><!--container-->\r\n",
  "components/epicContent/google-translate": "<!-- google translate code to be placed where you want it to show-->\n<div id=\"google_translate_element\"></div><script type=\"text/javascript\">\nfunction googleTranslateElementInit() {\n  new google.translate.TranslateElement({pageLanguage: 'en', includedLanguages: 'ar,bn,es,fr,hi,ja,pa,pt,ru,zh-CN', layout: google.translate.TranslateElement.InlineLayout.SIMPLE}, 'google_translate_element');\n}\n</script><script type=\"text/javascript\" src=\"//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit\"></script>\n",
  "components/custom/top-bar-additonal-content": "<li class=\"additional-top-bar-line\">\r\n    <a href=\"{{theme_settings.epic_content_help_center_url}}\"><i class=\"fas fa-user-clock\"></i><span class=\"topBar-icon-title\">Help</span></a>\r\n</li>\r\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\r\n<ul class=\"navUser-section\">\r\n    <li class=\"navUser-item\">\r\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\r\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n            {{#each currency_selector.currencies}}\r\n            <li class=\"dropdown-menu-item\">\r\n                <a href=\"{{{switch_url}}}\"\r\n                   data-currency-code=\"{{{code}}}\"\r\n                   {{#if cart_currency_switch_url.length '>' 0 }}\r\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\r\n                   {{/if}}\r\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\r\n                >\r\n                {{#if is_active}}\r\n                <strong>{{name}}</strong>\r\n                {{else}}\r\n                {{name}}\r\n                {{/if}}\r\n                </a>\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n    </li>\r\n</ul>\r\n{{/if}}\r\n\r\n"
}
