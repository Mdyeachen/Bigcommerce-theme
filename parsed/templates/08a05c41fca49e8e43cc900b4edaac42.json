{
  "ysw/widgets/YSW Mega Nav Dropdown Items/widget": "<style>\n    .ysw-w-mega-nav-dropdown-items.ysw-w-mega-nav-dropdown-items--{{_.id}} {\n        margin-left: {{margin_left_value.value}}{{margin_left_value.type}};\n        width: {{width_value.value}}{{width_value.type}};\n    }\n\n    {{#if enable_header '==' false}}\n        .ysw-w-mega-nav-dropdown-items.ysw-w-mega-nav-dropdown-items--{{_.id}} .ysw-w-mega-nav-dropdown-items__list  {\n            margin-top: 35px;\n        }\n    {{/if}}\n    {{#if enable_header}}\n        @media (max-width: 1024px) {\n            .ysw-w-mega-nav-dropdown-items.ysw-w-mega-nav-dropdown-items--{{_.id}} {\n                margin-top: 10px;\n            }\n        }\n    {{/if}}\n</style>\n\n<div class=\"ysw-w-mega-nav-dropdown-items ysw-w-mega-nav-dropdown-items--{{_.id}}\">\n    {{#if enable_header}}\n        <div class=\"ysw-w-mega-nav-dropdown-items__header\">\n            {{#if heading}}\n                <h3 class=\"ysw-w-mega-nav-dropdown-items__title\">{{heading}}{{#or (if link_url) (if link_text)}} | {{/or}}</h3>\n            {{/if}}\n            {{#or (if link_url) (if link_text)}}\n                <a href=\"{{link_url}}\" class=\"ysw-w-mega-nav-dropdown-items__link\">\n                    {{link_text}}\n                </a>\n            {{/or}}\n        </div>\n    {{/if}}\n    <ul class=\"ysw-w-mega-nav-dropdown-items__list\">\n        {{#each menu_list}}\n            {{#if hide '!=' true}}\n                <li class=\"ysw-w-mega-nav-dropdown-items__item\">\n                    <a href=\"{{url}}\" class=\"ysw-w-mega-nav-dropdown-items__item-link\">\n                        {{title}}\n                    </a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n"
}
