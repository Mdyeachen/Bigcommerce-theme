{
  "components/common/subscription-form": "<h5>\r\n    {{lang 'newsletter.subscribe'}}\r\n</h5>\r\n\r\n<form class=\"form mb-0\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field mb-0\">\r\n            <label\r\n                class=\"form-label is-srOnly\"\r\n                for=\"nl_email\"\r\n            >\r\n                {{lang 'common.email_address'}}\r\n            </label>\r\n            <div class=\"form-prefixPostfix wrap subcription-style\">\r\n                <input\r\n                    class=\"form-input\"\r\n                    id=\"nl_email\"\r\n                    name=\"nl_email\"\r\n                    type=\"email\"\r\n                    value=\"{{customer.email}}\"\r\n                    placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\r\n                    aria-describedby=\"alertBox-message-text\"\r\n                    required\r\n                    >\r\n                <input\r\n                    class=\"form-prefixPostfix-button--postfix\"\r\n                    type=\"submit\"\r\n                    value=\"{{lang 'newsletter.subscribe_submit'}}\"\r\n                >\r\n            </div>\r\n\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n\r\n"
}
