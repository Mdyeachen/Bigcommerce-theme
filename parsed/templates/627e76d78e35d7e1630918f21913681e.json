{
  "ysw/addons/mega-nav/mega-nav-list": "{{#if children}}\n    <a class=\"ysw-c-mega-nav__item-action navPages-action has-subMenu {{#if is_active}}activePage{{/if}}\"\n        href=\"{{url}}\"\n    >\n        {{name}}\n        <i class=\"ysw-c-mega-nav__item-icon icon navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg>\n                <use xlink:href=\"#icon-chevron-down\" />\n            </svg>\n        </i>\n    </a>\n\n    <div class=\"ysw-c-mega-nav__sub-menu navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n        {{{region name=(concat (concat \"navigation-menu-\" id) \"--global\")}}}\n    </div>\n{{else}}\n        <a class=\"ysw-c-mega-nav__item-action navPages-action{{#if is_active}} activePage{{/if}}\"\n            href=\"{{url}}\"\n            aria-label=\"{{name}}\"\n        >\n            {{name}}\n        </a>\n{{/if}}\n"
}
