{
  "components/epicHeader/logo-bar": "<div class=\"container-fluid\">\r\n    <div class=\"row align-items-center\">\r\n        {{#if theme_settings.logo-position '==' 'left'}}\r\n            <div class=\"col-lg\">\r\n                {{#if page_type '===' 'default'}}\r\n                    <div class=\"site-logo header-logo--left\">\r\n                        {{> components/common/store-logo}}\r\n                        <!--<a href = \"#\" class=\"header-logo__link luxeModz\">\r\n                            <img src=\"https://cdn11.bigcommerce.com/s-m5ct3euedw/images/stencil/original/image-manager/luxemodz.png\" alt=\"Luxe Modz\" style=\"max-height: 65px;\">\r\n                        </a> -->\r\n                    </div>\r\n                {{else}}\r\n                    <div class=\"site-logo header-logo--left\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                {{/if}}\r\n            </div>\r\n            <div class=\"col-lg\">\r\n                <div>{{> components/epicHeader/header-tabs}}</div>\r\n                \r\n                {{#if theme_settings.epic_hide_search_navigation}}\r\n                    {{> components/epicHeader/search}}\r\n                {{/if}}\r\n            </div>\r\n            <div class=\"col-lg\">\r\n                    <ul class=\"navUser-section top-bar-menu topBar-custom\">\r\n                        <!--<li>\r\n                            <a href=\"https://bodyjewelry.com/luxe-modz/\">\r\n                                <img src=\"https://cdn11.bigcommerce.com/s-m5ct3euedw/images/stencil/original/image-manager/luxe-logo.jpg\" alt=\"luxe modz Body Jewlery\" height=\"30\" width=\"30\"><span class=\"topBar-icon-title\">Luxe Modz</span>\r\n                            </a>\r\n                        </li>-->\r\n                        {{#if theme_settings.show-help-center}}\r\n                            {{> components/custom/top-bar-additonal-content}}\r\n                        {{/if}}\r\n                        {{#if customer}}\r\n                        <li>\r\n                            <a href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\">\r\n                                <i class=\"fas fa-user-circle\"></i><span class=\"topBar-icon-title\">My Account</span>\r\n                            </a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"{{urls.auth.logout}}\"><i class=\"fas fa-user-alt-slash\"></i><span class=\"topBar-icon-title\">Log Out</span></a>\r\n                        </li>\r\n                        {{else}}\r\n                        <li>\r\n                           <!-- <a href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a> -->\r\n                            <a href=\"javascript:void(0);\" data-dropdown=\"login-drop\" aria-controls=\"login-drop\" aria-expanded=\"false\"><i class=\"fas fa-user\"></i><span class=\"topBar-icon-title\">{{lang 'common.login'}} / {{lang 'common.sign_up'}}</span></a> \r\n                            <div id=\"login-drop\" data-dropdown-content class=\"f-dropdown login-drop-contennt content cusStyle\" aria-autoclose=\"false\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                                <h4>Returning Customer</h4>\r\n                                <form class=\"login-form form\" action=\"/login.php?action=check_login\" method=\"post\" name=\"FormLogin\">\r\n                                    <div class=\"form-field form-field--input form-field--inputEmail\">\r\n                                        <label class=\"form-label\" for=\"login_email\">Email Address:</label>\r\n                                        <input class=\"form-input\" name=\"login_email\" id=\"login_email\" type=\"email\">\r\n                                    </div>\r\n                                    <div class=\"form-field form-field--input form-field--inputPassword\">\r\n                                        <label class=\"form-label\" for=\"login_pass\">Password:</label>\r\n                                        <input class=\"form-input\" id=\"login_pass\" type=\"password\" name=\"login_pass\">\r\n                                    </div>\r\n                                    <div class=\"form-actions\">\r\n                                        <input type=\"submit\" class=\"button button--primary\" value=\"Sign in\">\r\n                                        <a class=\"forgot-password\" href=\"/login.php?action=reset_password\">Forgot your password?</a>\r\n                                    </div>\r\n                                </form>\r\n                                {{#if settings.account_creation_enabled}}\r\n                                <div class=\"signup-link\">\r\n                                    <h4>New Customer</h4>\r\n                                    <p>Registering provides you with personalized services, including: wish list, order status and promotions.</p>\r\n                                    <a class=\"button\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\r\n                                </div>\r\n                                {{/if}}\r\n                            </div>\r\n                        </li>\r\n                        <!--<li class=\"signup\">\r\n                            {{#if settings.account_creation_enabled}}\r\n                            <a href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\r\n                            {{/if}}\r\n                        </li>-->\r\n                        {{/if}}\r\n                        {{#if settings.gift_certificates_enabled}}\r\n                            <li>\r\n                                <a href=\"{{urls.gift_certificate.purchase}}\"><i class=\"fas fa-gift\"></i><span class=\"topBar-icon-title\">Gift Certificates</span></a>\r\n                            </li>\r\n                        {{/if}}\r\n                        <li class=\"top-bar-cart navUser-item--cart\">\r\n                            <a\r\n                                class=\"\"\r\n                                data-cart-preview\r\n                                data-dropdown=\"cart-preview-dropdown\"\r\n                                data-options=\"align:right\"\r\n                                href=\"{{urls.cart}}\">\r\n                                <span class=\"navUser-item-cartLabel\"><i class=\"far fa-shopping-cart\" aria-hidden=\"true\"></i></span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span><span class=\"topBar-icon-title\">Cart</span>\r\n                            </a>\r\n                            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"javascript:void(0)\" id=\"trigger-overlay\" class=\"dropdown-button\"><i class=\"fas fa-store-alt\"></i><span class=\"topBar-icon-title\">Shop Info</span></a>\r\n                        </li>\r\n                        <li><a href=\"tel:{{settings.phone_number}}\"><i class=\"fas fa-phone-square\"></i><span class=\"topBar-icon-title\">1.888.242.6166</span></a></li>\r\n                        <li><a href=\"https://bodyjewelry.com/shipping-info/\"><i class=\"fas fa-shipping-fast\"></i><span class=\"topBar-icon-title\">We Ship Worldwide</span></a></li>\r\n                        <li><a href=\"https://track.shipstation.com/f/r/b/SW0pkJ9U8l10lTf0rc5rlA%3D%3D#\"><i class=\"fas fa-undo fa-fw\"></i><span class=\"topBar-icon-title\">Tracking</span></a></li>\r\n                    </ul>\r\n                    \r\n                    <style>\r\n                    .cusStyle.f-dropdown.is-open{\r\n                        top: 2rem !important;\r\n                    }\r\n                    \r\n                    .dropdown-menu{\r\n                        min-width: min-content;\r\n                    }\r\n                    \r\n                    .site-logo.header-logo--left{\r\n                        display: flex;\r\n                        justify-content: space-between;\r\n                        \r\n                    }\r\n                    \r\n                  \r\n                    .top-bar-menu.topBar-custom {\r\n                      display: flex;\r\n                      list-style: none;\r\n                      justify-content: center;\r\n                      float: none;\r\n                      margin: 0;\r\n                      align-items: center;\r\n                    }\r\n                    .top-bar-menu.topBar-custom li {\r\n                      padding: 0 12px;\r\n                      font-size: 22px;\r\n                      position: relative;\r\n                    }\r\n                    \r\n                    .topBar-icon-title {\r\n                    \tfont-size: 14px;\r\n                    \tposition: absolute;\r\n                    \tbottom: -200%;\r\n                    \tbackground: #fff;\r\n                    \tpadding: 0 8px;\r\n                    \tborder: 1px solid;\r\n                    \tborder-radius: 6px;\r\n                    \tz-index: -1;\r\n                    \tleft: 50%;\r\n                    \ttransform: translateX(-50%);\r\n                    \twidth: max-content;\r\n                    \ttransition: 0.2s;\r\n                    \topacity: 0;\r\n                    \tvisibility: hidden;\r\n                    }\r\n                                        \r\n                    .top-bar-menu.topBar-custom li:hover .topBar-icon-title {\r\n                    \tbottom: -90%;\r\n                    \tz-index: 11;\r\n                    \topacity: 1;\r\n                    \tvisibility: visible;\r\n                    }\r\n                    \r\n                    .topBar-icon-title::before {\r\n                        position: absolute;\r\n                        top: -5px;\r\n                        left: 50%;\r\n                        transform: rotate(45deg);\r\n                        height: 10px;\r\n                        width: 10px;\r\n                        content: \"\";\r\n                        margin-right: ;\r\n                        background: #fff;\r\n                        border-left: 1px solid;\r\n                        border-top: 1px solid;\r\n                    }\r\n                    .topBar-icon-title:nth-last-child(1) {\r\n                      transform: translateX(-80%);\r\n                    }\r\n                    .topBar-icon-title:nth-last-child(1):before {\r\n                      left: auto;\r\n                      right: 20%;\r\n                    }\r\n                    \r\n                    .countPill {\r\n                    \tposition: absolute;\r\n                    \tright: 0;\r\n                    }\r\n                    \r\n                    @media (max-width: 991px) {\r\n                        .header-search {\r\n                        \tpadding: 5px 25px;\r\n                        }\r\n                        .navUser-section.top-bar-menu.topBar-custom {\r\n                        \tjustify-content: center;\r\n                        }\r\n                    }\r\n                    </style>\r\n\r\n\r\n                {{#if theme_settings.show-payment-icons-header}}\r\n                    <div class=\"payment-icon text-right\">\r\n                        {{> components/epicHeader/payment-icon}}\r\n                    </div>\r\n                {{/if}}\r\n            </div>\r\n        {{/if}}\r\n\r\n        {{#if theme_settings.logo-position '==' 'center'}}\r\n            <div class=\"col-lg-4\">\r\n                <div class=\"call-us text-left\">\r\n                    <h3>\r\n                        <a href=\"tel:{{settings.phone_number}}\">\r\n                            {{settings.phone_number}}\r\n                        </a>\r\n                    </h3>\r\n                    {{#if theme_settings.header-shop-address '!==' \"\" }}\r\n                        <p>{{{theme_settings.header-shop-address}}}</p>\r\n                    {{/if}}\r\n                </div>\r\n                {{#if theme_settings.show-payment-icons-header}}\r\n                    <div class=\"payment-icon text-right\">\r\n                        {{> components/epicHeader/payment-icon}}\r\n                    </div>\r\n                {{/if}}\r\n            </div>\r\n            <div class=\"col-lg-4\">\r\n                {{#if page_type '===' 'default'}}\r\n                    <h1 class=\"site-logo header-logo--center\">\r\n                        {{> components/common/store-logo}}\r\n                    </h1>\r\n                {{else}}\r\n                    <div class=\"site-logo header-logo--center\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                {{/if}}\r\n            </div>\r\n            <div class=\"col-lg-4\">\r\n                {{#if theme_settings.epic_hide_search_navigation}}\r\n                    {{> components/epicHeader/search}}\r\n                {{/if}}\r\n            </div>\r\n            {{> components/epicHeader/header-tabs}}\r\n        {{/if}}\r\n\r\n        {{#if theme_settings.logo-position '==' 'right'}}\r\n            <div class=\"col-lg-3\">\r\n                <div class=\"call-us text-left\">\r\n                    <h3>\r\n                        <a href=\"tel:{{settings.phone_number}}\">\r\n                            {{settings.phone_number}}\r\n                        </a>\r\n                    </h3>\r\n                    {{#if theme_settings.header-shop-address '!==' \"\" }}\r\n                        <p>{{{theme_settings.header-shop-address}}}</p>\r\n                    {{/if}}\r\n                </div>\r\n                {{#if theme_settings.show-payment-icons-header}}\r\n                    <div class=\"payment-icon text-left\">\r\n                        {{> components/epicHeader/payment-icon}}\r\n                    </div>\r\n                {{/if}}\r\n            </div>\r\n            <div class=\"col-lg-6\">\r\n                {{> components/epicHeader/header-tabs}}\r\n                {{#if theme_settings.epic_hide_search_navigation}}\r\n                    {{> components/epicHeader/search}}\r\n                {{/if}}\r\n            </div>\r\n            <div class=\"col-lg-3\">\r\n                {{#if page_type '===' 'default'}}\r\n                    <h1 class=\"site-logo header-logo--right\">\r\n                        {{> components/common/store-logo}}\r\n                    </h1>\r\n                {{else}}\r\n                    <div class=\"site-logo header-logo--right\">\r\n                        {{> components/common/store-logo}}\r\n                    </div>\r\n                {{/if}}\r\n            </div>\r\n        {{/if}}\r\n\r\n    </div><!--row-->\r\n    {{#if theme_settings.epic_hide_search_navigation}}\r\n        <section data-prevent-quick-search-close  class=\"quickSearchResults quick-search\" data-bind=\"html: results\"></section>\r\n    {{/if}}\r\n</div><!--container-fluid-->\r\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\" style=\"width: 100%;\">\r\n    {{#if settings.store_logo.image}}\r\n        {{#if theme_settings.logo_size '===' 'original'}}\r\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"Luxe Modz Body Jewelry\" title=\"{{settings.store_logo.title}}\">\r\n        {{else}}\r\n            <div class=\"header-logo-image-container\">\r\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n            </div>\r\n        {{/if}}\r\n    {{else}}\r\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n",
  "components/epicHeader/header-tabs": "{{#if theme_settings.show_header_tabs}}\r\n{{#if theme_settings.logo-position '==' 'center'}}\r\n<div class=\"col-6 mr-auto ml-auto mt-3\">\r\n{{/if}}\r\n    <ul class=\"row header-col\">\r\n        <li class=\"col\">\r\n            <a class=\"{{#if theme_settings.show_header_tabs}}shipping-drop{{else}}no-drop{{/if}}\" href=\"JavaScript:void(0)\">\r\n                <h4><i class=\"{{theme_settings.epic_header_shipping_icon}}\"></i> {{theme_settings.epic_header_shipping_heading}}</h4>\r\n            </a>\r\n        </li>\r\n        <li class=\"col\">\r\n            <a class=\"{{#if theme_settings.show_header_tabs}}price-drop{{else}}no-drop{{/if}}\" href=\"JavaScript:void(0)\">\r\n                <h4><i class=\"{{theme_settings.epic_header_custom_design_icon}}\"></i> {{theme_settings.epic_header_custom_design_heading}}</h4>\r\n            </a>\r\n        </li>\r\n        <li class=\"col\">\r\n            <a class=\"{{#if theme_settings.show_header_tabs}}support-drop{{else}}no-drop{{/if}}\" href=\"JavaScript:void(0)\">\r\n                <h4><i class=\"{{theme_settings.epic_header_happiness_guaranteed_icon}}\"></i> {{theme_settings.epic_header_happiness_heading}}</h4>\r\n            </a>\r\n        </li>\r\n    </ul>\r\n    <div id=\"free-shipping\" class=\"header-col-drop\">\r\n        {{> components/custom/header-tabs/tab-one }}\r\n    </div>\r\n    <div id=\"price-promise\" class=\"header-col-drop\">\r\n        {{> components/custom/header-tabs/tab-two }}\r\n    </div>\r\n    <div id=\"online-support\" class=\"header-col-drop\">\r\n        {{> components/custom/header-tabs/tab-three }}\r\n    </div>\r\n    <div class=\"divider-15\"></div>\r\n{{#if theme_settings.logo-position '==' 'center'}}\r\n</div>\r\n{{/if}}\r\n{{/if}}\r\n",
  "components/epicHeader/search": "<div class=\"header-search\">\n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\"  action=\"{{urls.search}}\">\n        <select name=\"category-search\" id=\"resizing_select\" class=\"browser-default top-select form-select\">\n            <option value=\"\" selected>All &nbsp;</option>\n            {{#each categories}}\n                <option data-category-url=\"{{url}}\" value=\"{{id}}\">{{name}} &nbsp;</option>\n            {{/each}}\n        </select>\n        <select id=\"width_tmp_select\">\n            <option id=\"width_tmp_option\"></option>\n        </select>\n        <div class=\"search-input-field\">\n            <input name=\"search_query_adv\" data-search-quick class=\"search_\" type=\"search\" autocomplete=\"off\" onfocus=\"this.placeholder=''\" onblur=\"this.placeholder='{{theme_settings.header-search-text}}'\" placeholder=\"{{theme_settings.header-search-text}}\">\n            <button  class=\"button btn btn2\" name=\"go\" value=\"{{lang 'epicHeader.search'}}\">\n                <i class=\"far fa-search\" aria-hidden=\"true\"></i>\n            </button>\n        </div>\n    </form>\n</div>\n",
  "components/epicHeader/payment-icon": "<ul>\n    {{#if theme_settings.epic_hide_lock_icon}}\n    <li><i class=\"fas fa-lock\"></i></li>\n    {{/if}}\n    \n    {{#if theme_settings.epic_hide_visa_icon}}\n    <li><i class=\"fab fa-cc-visa\"></i></li>\n    {{/if}}\n    \n    {{#if theme_settings.epic_hide_master_icon}}\n    <li><i class=\"fab fa-cc-mastercard\"></i></li>\n    {{/if}}\n\n    {{#if theme_settings.epic_hide_discover_icon}}\n    <li><i class=\"fab fa-cc-discover\"></i></li>\n    {{/if}}\n\n    {{#if theme_settings.epic_hide_amex_icon}}\n    <li><i class=\"fab fa-cc-amex\"></i></li>\n    {{/if}}\n\n    {{#if theme_settings.epic_hide_stripe_icon}}\n    <li><i class=\"fab fa-cc-stripe\"></i></li>\n    {{/if}}\n    \n    {{#if theme_settings.epic_hide_paypal_icon}}\n    <li><i class=\"fab fa-cc-paypal\"></i></li>\n    {{/if}}\n    \n    {{#if theme_settings.epic_hide_apple_icon}}\n    <li><i class=\"fab fa-apple-pay\"></i></li>\n    {{/if}}\n    \n    {{#if theme_settings.epic_hide_android_icon}}\n    <li><i class=\"fab fa-android\"></i></li>\n    {{/if}}\n    \n    {{#if theme_settings.epic_hide_diners_icon}}\n    <li><i class=\"fab fa-cc-diners-club\"></i></li>\n    {{/if}}\n\n    {{#if theme_settings.epic_hide_amazon_icon}}\n    <li><i class=\"fab fa-amazon\"></i></li>\n    {{/if}}\n\n    {{#if theme_settings.epic_hide_gwallet_icon}}\n    <li><i class=\"fab fa-google-wallet\"></i></li>\n    {{/if}}\n\n    {{#if theme_settings.epic_hide_jcb_icon}}\n    <li><i class=\"fab fa-cc-jcb\"></i></li>\n    {{/if}}\n</ul>\n",
  "components/custom/top-bar-additonal-content": "<li class=\"additional-top-bar-line\">\r\n    <a href=\"{{theme_settings.epic_content_help_center_url}}\"><i class=\"fas fa-user-clock\"></i><span class=\"topBar-icon-title\">Help</span></a>\r\n</li>\r\n",
  "components/custom/header-tabs/tab-two": "<p>\n    {{theme_settings.epic_header_custom_design_body}}\n</p>\n",
  "components/custom/header-tabs/tab-one": "<p>\n    {{theme_settings.epic_header_shipping_body}}\n</p>\n",
