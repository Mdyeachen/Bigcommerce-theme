{
  "ysw/components/products/description-tabs": "<div class=\"ysw-c-description-tabs\">\r\n    <details class=\"ysw-c-accordion\" open=\"\">\r\n        <summary class=\"ysw-c-accordion__title\">{{lang 'products.description'}}</summary>\r\n        <div class=\"ysw-c-accordion__panel\">\r\n            <div class=\"ysw-c-accordion__panel-content\">\r\n                {{{product.description}}}\r\n            </div>\r\n        </div>\r\n    </details>\r\n    {{#if theme_settings.exta_product_tab '==' true}}\r\n    <details class=\"ysw-c-accordion\" open=\"\">\r\n        <summary class=\"ysw-c-accordion__title\">Info</summary>\r\n        <div class=\"ysw-c-accordion__panel\">\r\n            <div class=\"ysw-c-accordion__panel-content\">\r\n                {{> components/custom/extra-product-tab}}\r\n            </div>\r\n        </div>\r\n    </details>\r\n    {{/if}}\r\n    {{#if product.warranty}}\r\n    <details class=\"ysw-c-accordion\">\r\n        <summary class=\"ysw-c-accordion__title\">{{lang 'products.warranty'}}</summary>\r\n        <div class=\"ysw-c-accordion__panel\">\r\n            <div class=\"ysw-c-accordion__panel-content\">\r\n                {{{product.warranty}}}\r\n            </div>\r\n        </div>\r\n    </details>\r\n    {{/if}}\r\n    {{#all product.custom_fields theme_settings.show_custom_fields_tabs}}\r\n    <details class=\"ysw-c-accordion\">\r\n        <summary class=\"ysw-c-accordion__title\">{{{sanitize theme_settings.pdp-custom-fields-tab-label}}}</summary>\r\n        <div class=\"ysw-c-accordion__panel\">\r\n            <div class=\"ysw-c-accordion__panel-content\">\r\n                <dl class=\"productView-info\">\r\n                    {{#each product.custom_fields}}\r\n                        <dt class=\"productView-info-name\">{{name}}:</dt>\r\n                        <dd class=\"productView-info-value\">{{{value}}}</dd>\r\n                    {{/each}}\r\n                </dl>\r\n            </div>\r\n        </div>\r\n    </details>\r\n    {{/all}}\r\n    <details class=\"ysw-c-accordion\">\r\n        <summary class=\"ysw-c-accordion__title\">Shipping Information</summary>\r\n        <div class=\"ysw-c-accordion__panel\">\r\n            <div class=\"ysw-c-accordion__panel-content review-des\" style=\"padding: 0 3px;\">\r\n                <div style=\"text-align: left;\" align=\"center\">\r\n                <table class=\"table tg product-tab-table\">\r\n                <tbody>\r\n                <tr>\r\n                <th class=\"tg-hgcj\">Shipping Method</th>\r\n                <th class=\"tg-amwm\">Handling Time</th>\r\n                <th class=\"tg-amwm\">Shipping Time </th>\r\n                <th class=\"tg-amwm\">Price</th></tr>\r\n                <tr>\r\n                <td class=\"tg-0vih\">USPS First Class</td>\r\n                <td class=\"tg-dzk6\">2-3 Business Days</td>\r\n                <td class=\"tg-dzk6\">3-7 Business Days</td>\r\n                <td class=\"tg-dzk6\">$4.75</td>\r\n                </tr>\r\n                <tr>\r\n                <td class=\"tg-amwm\">USPS Priority Mail</td>\r\n                <td class=\"tg-baqh\">2-3 Business Days</td>\r\n                <td class=\"tg-baqh\">2-4 Business Days</td>\r\n                <td class=\"tg-baqh\">$9.75</td>\r\n                </tr>\r\n                <tr>\r\n                <td class=\"tg-amwm\">USPS Mail Express</td>\r\n                <td class=\"tg-baqh\">2-3 Business Days</td>\r\n                <td class=\"tg-baqh\">1-2 Business Days (Express)</td>\r\n                <td class=\"tg-baqh\">$29.75</td>\r\n                </tr>\r\n                <tr>\r\n                <td class=\"tg-amwm\">UPS International Express</td>\r\n                <td class=\"tg-baqh\">2-3 Business Days</td>\r\n                <td class=\"tg-baqh\">3-5 Business Days</td>\r\n                <td class=\"tg-baqh\">$19.95</td>\r\n                </tr>\r\n                <tr>\r\n                <td class=\"tg-amwm\">International Mail Ground</td>\r\n                <td class=\"tg-baqh\">2-3 Business Days</td>\r\n                <td class=\"tg-baqh\">7-10 Business Day</td>\r\n                <td class=\"tg-baqh\">$6.75</td>\r\n                </tr>\r\n                </tbody>\r\n                </table>\r\n                </div>\r\n                <style>\r\n                .ysw-c-accordion__panel-content h1,\r\n                .ysw-c-accordion__panel-content h2,\r\n                .ysw-c-accordion__panel-content h3{\r\n                \tfont-size: 18px;\r\n                    margin-top: 10px;\r\n                }\r\n                .product-tab-table.table{\r\n                    margin-top: 10px;\r\n                }\r\n                \r\n                .product-tab-table.table tr td, .product-tab-table.table tr th {\r\n                    padding: 5px 0px !important;\r\n                    text-align: center;\r\n                \tborder: 1px solid #e1e1e1;\r\n                }\r\n                @media (min-width:991px) {\r\n                    .product-tab-table.table tr td, .product-tab-table.table tr th {\r\n                \tpadding: 10px 10px !important;\r\n                \ttext-align: left;\r\n                    }\r\n                    \r\n                }\r\n                @media (max-width:800px) {\r\n                    .product-tab-table.table tr td, .product-tab-table.table tr th {\r\n                \tpadding: 10px 10px !important;\r\n                \ttext-align: left;\r\n                    }\r\n                    \r\n                }\r\n                @media (max-width:450px) {\r\n                    .product-tab-table.table tr td, .product-tab-table.table tr th {\r\n                \tpadding: 5px 0px !important;\r\n                    }\r\n                    \r\n                }\r\n                </style>\r\n            </div>\r\n        </div>\r\n    </details>\r\n    <details class=\"ysw-c-accordion\">\r\n        <summary class=\"ysw-c-accordion__title\">Warranty Information</summary>\r\n        <div class=\"ysw-c-accordion__panel\">\r\n            <div class=\"ysw-c-accordion__panel-content\">\r\n                <p>3 & 4-Year Warranty</p>\r\n                <p>Whether you have a 3-year or 4-year warranty, you are guaranteed to have your jewelry replaced at no extra cost. Beginning from the date of purchase, replacement is guaranteed if your jewelry is damaged or broken.</p>\r\n        \r\n                <p>To make use of your warranty, please notify us via email at <a href=\"mailto:support@bodyjewelry.com\">support@bodyjewelry.com</a> or call us toll-free at 1-888-242-6166.</p>\r\n        \r\n                <p>For more details, <a href=\"https://bodyjewelry.com/warranty-policy/\">click here</a> to view our full warranty policy.</p>\r\n                <p><b>Please note: Shipping costs are not covered by the warranty.</b></p>\r\n            </div>\r\n        </div>\r\n    </details>\r\n</div>\r\n",
  "components/custom/extra-product-tab": "<p>Hello World</p>\n"
}
