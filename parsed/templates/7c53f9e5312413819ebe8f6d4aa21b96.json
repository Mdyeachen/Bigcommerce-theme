{
  "components/common/navigation": "<nav class=\"navUser\">\n    {{> components/common/currency-selector}}\n\n    <ul class=\"navUser-section navUser-section--alt\">\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\"\n                   aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\"\n                >\n                    <span class=\"navUser-action-divider\">\n                        {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                    </span>\n                </a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action navUser-item--compare\"\n               href=\"{{urls.compare}}\"\n               data-compare-nav\n               aria-label=\"{{lang 'common.compare'}}\"\n            >\n                {{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span>\n            </a>\n        </li>\n        {{#if theme_settings.social_icon_placement_top}}\n            <li class=\"navUser-item navUser-item--social\">\n                {{> components/common/social-links}}\n            </li>\n            <li class=\"navUser-item navUser-item--divider\">|</li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <button class=\"navUser-action navUser-action--quickSearch\"\n               type=\"button\"\n               id=\"quick-search-expand\"\n               data-search=\"quickSearch\"\n               aria-controls=\"quickSearch\"\n               aria-label=\"{{lang 'common.search'}}\"\n            >\n                {{lang 'common.search'}}\n            </button>\n        </li>\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action\"\n                   href=\"{{urls.gift_certificate.purchase}}\"\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\n                >\n                    {{lang 'common.gift_cert'}}\n                </a>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item navUser-item--account\">\n            {{#if customer}}\n                <a class=\"navUser-action\"\n                   href=\"{{urls.account.index}}\"\n                   aria-label=\"{{lang 'common.account'}}\"\n                >\n                    {{lang 'common.account'}}\n                </a>\n                <a class=\"navUser-action\"\n                   href=\"{{urls.auth.logout}}\"\n                   aria-label=\"{{lang 'common.logout'}}\"\n                >\n                    {{lang 'common.logout'}}\n                </a>\n            {{else}}\n                <a class=\"navUser-action\"\n                   href=\"{{urls.auth.login}}\"\n                   aria-label=\"{{lang 'common.login'}}\"\n                >\n                    {{lang 'common.login'}}\n                </a>\n                {{#if settings.account_creation_enabled}}\n                    <span class=\"navUser-or\">{{lang 'common.or'}}</span>\n                    <a class=\"navUser-action\"\n                       href=\"{{urls.auth.create_account}}\"\n                       aria-label=\"{{lang 'common.sign_up'}}\"\n                    >\n                        {{lang 'common.sign_up'}}\n                    </a>\n                {{/if}}\n            {{/if}}\n        </li>\n        <li class=\"navUser-item navUser-item--cart\">\n            <a class=\"navUser-action\"\n               data-cart-preview\n               data-dropdown=\"cart-preview-dropdown\"\n               data-options=\"align:right\"\n               href=\"{{urls.cart}}\"\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\n            >\n                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span>\n                <span class=\"countPill cart-quantity\"></span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n    </ul>\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\n        {{> components/common/quick-search name='nav-quick-search'}}\n    </div>\n</nav>\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\r\n<ul class=\"navUser-section\">\r\n    <li class=\"navUser-item\">\r\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\r\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n            {{#each currency_selector.currencies}}\r\n            <li class=\"dropdown-menu-item\">\r\n                <a href=\"{{{switch_url}}}\"\r\n                   data-currency-code=\"{{{code}}}\"\r\n                   {{#if cart_currency_switch_url.length '>' 0 }}\r\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\r\n                   {{/if}}\r\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\r\n                >\r\n                {{#if is_active}}\r\n                <strong>{{name}}</strong>\r\n                {{else}}\r\n                {{name}}\r\n                {{/if}}\r\n                </a>\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n    </li>\r\n</ul>\r\n{{/if}}\r\n\r\n",
  "components/common/social-links": "{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                <a\r\n                    class=\"icon icon--{{display_name}}\"\r\n                    href=\"{{url}}\"\r\n                    target=\"_blank\"\r\n                    rel=\"noopener\"\r\n                >\r\n                    <svg>\r\n                        <use xlink:href=\"#icon-{{display_name}}\" />\r\n                    </svg>\r\n                </a>\r\n            </li>\r\n        {{/each}}\r\n        <!-- linkedin add raw code components/common/social-links.html -->\r\n        \r\n        <li class=\"socialLinks-item\">\r\n            <a\r\n                class=\"icon icon--linkedin\"\r\n                href=\"https://www.linkedin.com/company/bodyjewelry.com/\"\r\n                target=\"_blank\"\r\n                rel=\"noopener\"\r\n            >\r\n                <svg>\r\n                    <use xlink:href=\"#icon-linkedin\" />\r\n                </svg>\r\n            </a>\r\n        </li>\r\n    </ul>\r\n{{/if}}\r\n",
  "components/common/quick-search": "<div class=\"container\">\r\n    <!-- <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form> -->\r\n    <form class=\"form\" action=\"{{urls.search}}\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\"\r\n                       {{#if data '===' 'false'}}{{else}}data-search-quick{{/if}}\r\n                       name=\"{{name}}\"\r\n                       id=\"{{name}}\"\r\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\r\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\r\n                       autocomplete=\"off\"\r\n                >\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n</div>\r\n"
}
