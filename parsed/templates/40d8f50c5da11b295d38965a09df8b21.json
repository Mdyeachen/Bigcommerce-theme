{
  "components/common/body": "<div id=\"main-content\" class=\"body{{#if page_type '===' 'default'}} home{{/if}}{{#if theme_settings.add-three-product-process}}{{#if page_type '==' 'product'}} pro-details-tab{{/if}}{{/if}}\" data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}} {{/block}}\r\n\t<div class=\"site-content{{#and (if page_type '!==' 'default' ) (if page_type '!==' 'category')}} container container-lg{{/and}}\">\r\n\t\t\t{{#block \"page\"}} {{/block}}\r\n\t</div>\r\n    {{> components/common/modal/modal}}\r\n    {{> components/common/alert/alert-modal}}\r\n\r\n</div>\r\n",
  "components/common/modal/modal": "<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\n    {{> components/common/modal/modal-close-btn}}\n    <div class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n",
  "components/common/alert/alert-modal": "<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\n    <div class=\"swal2-icon swal2-error swal2-icon-show\"><span class=\"swal2-x-mark\"><span class=\"swal2-x-mark-line-left\"></span><span class=\"swal2-x-mark-line-right\"></span></span></div>\n\n    <div class=\"modal-content\"></div>\n\n    <div class=\"button-container\"><button type=\"button\" class=\"confirm button\" data-reveal-close>OK</button></div>\n</div>\n",
  "components/common/modal/modal-close-btn": "<button class=\"modal-close\"\n        type=\"button\"\n        title=\"{{lang 'common.close'}}\"\n        {{additional_attr}}\n>\n    <span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\n    <span aria-hidden=\"true\">&#215;</span>\n</button>\n"
}
