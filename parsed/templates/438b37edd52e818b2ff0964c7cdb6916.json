{
  "components/epicContent/home-content-snippet": "{{#if theme_settings.display-snippets-carousel}}\n<div\n    class=\"content-snippet-carousel\"\n    data-slick='{\n        \"dots\": false,\n        \"slidesToShow\": 4,\n        \"slidesToScroll\": 1,\n        \"autoplay\": true,\n        \"lazyLoad\": \"anticipated\",\n        \"slide\": \".content-snippet-carousel__item\",\n        \"prevArrow\": \".js-hero-prev-arrow\",\n        \"nextArrow\": \".js-hero-next-arrow\",\n        \"responsive\": [\n            {\n                \"breakpoint\": 992,\n                \"settings\": {\n                    \"slidesToScroll\": 1,\n                    \"slidesToShow\": 3\n                }\n            },\n            {\n                \"breakpoint\": 768,\n                \"settings\": {\n                    \"slidesToScroll\": 1,\n                    \"slidesToShow\": 2\n                }\n            },\n            {\n                \"breakpoint\": 576,\n                \"settings\": {\n                    \"slidesToScroll\": 1,\n                    \"slidesToShow\": 1\n                }\n            }\n        ]\n    }'\n>\n    <button\n        aria-label=\"{{lang 'carousel.arrowAriaLabel'}}\"\n        class=\"js-hero-prev-arrow slick-prev slick-arrow\"\n    ></button>\n\n    {{#if theme_settings.background-image-snippet-1 \"!==\" \"\"}}\n        <div class=\"content-snippet-carousel__item content-snippet-carousel__item--1\">\n            <a\n                href=\"{{theme_settings.btn-url-snippet-1}}\"\n                title=\"{{theme_settings.btn-text-snippet-1}}\"\n            >\n                <img\n                    src=\"{{getImageManagerImage theme_settings.background-image-snippet-1}}\"\n                    alt=\"{{theme_settings.btn-text-snippet-1}}\"\n                />\n            </a>\n        </div>\n    {{/if}}\n\n    {{#if theme_settings.background-image-snippet-2 \"!==\" \"\"}}\n        <div class=\"content-snippet-carousel__item content-snippet-carousel__item--2\">\n            <a\n                href=\"{{theme_settings.btn-url-snippet-2}}\"\n                title=\"{{theme_settings.btn-text-snippet-2}}\"\n            >\n                <img\n                    src=\"{{getImageManagerImage theme_settings.background-image-snippet-2}}\"\n                    alt=\"{{theme_settings.btn-text-snippet-2}}\"\n                />\n            </a>\n        </div>\n    {{/if}}\n\n    {{#if theme_settings.background-image-snippet-3 \"!==\" \"\"}}\n        <div class=\"content-snippet-carousel__item content-snippet-carousel__item--3\">\n            <a\n                href=\"{{theme_settings.btn-url-snippet-3}}\"\n                title=\"{{theme_settings.btn-text-snippet-3}}\"\n            >\n                <img\n                    src=\"{{getImageManagerImage theme_settings.background-image-snippet-3}}\"\n                    alt=\"{{theme_settings.btn-text-snippet-3}}\"\n                />\n            </a>\n        </div>\n    {{/if}}\n\n    {{#if theme_settings.background-image-snippet-4 \"!==\" \"\"}}\n        <div class=\"content-snippet-carousel__item content-snippet-carousel__item--4\">\n            <a\n                href=\"{{theme_settings.btn-url-snippet-4}}\"\n                title=\"{{theme_settings.btn-text-snippet-4}}\"\n            >\n                <img\n                    src=\"{{getImageManagerImage theme_settings.background-image-snippet-4}}\"\n                    alt=\"{{theme_settings.btn-text-snippet-4}}\"\n                />\n            </a>\n        </div>\n    {{/if}}\n\n    {{#if theme_settings.background-image-snippet-5 \"!==\" \"\"}}\n        <div class=\"content-snippet-carousel__item content-snippet-carousel__item--5\">\n            <a\n                href=\"{{theme_settings.btn-url-snippet-5}}\"\n                title=\"{{theme_settings.btn-text-snippet-5}}\"\n            >\n                <img\n                    src=\"{{getImageManagerImage theme_settings.background-image-snippet-5}}\"\n                    alt=\"{{theme_settings.btn-text-snippet-5}}\"\n                />\n            </a>\n        </div>\n    {{/if}}\n\n    {{#if theme_settings.background-image-snippet-6 \"!==\" \"\"}}\n        <div class=\"content-snippet-carousel__item content-snippet-carousel__item--6\">\n            <a\n                href=\"{{theme_settings.btn-url-snippet-6}}\"\n                title=\"{{theme_settings.btn-text-snippet-6}}\"\n            >\n                <img\n                    src=\"{{getImageManagerImage theme_settings.background-image-snippet-6}}\"\n                    alt=\"{{theme_settings.btn-text-snippet-6}}\"\n                />\n            </a>\n        </div>\n    {{/if}}\n\n    {{#if theme_settings.background-image-snippet-7 \"!==\" \"\"}}\n        <div class=\"content-snippet-carousel__item content-snippet-carousel__item--7\">\n            <a\n                href=\"{{theme_settings.btn-url-snippet-7}}\"\n                title=\"{{theme_settings.btn-text-snippet-7}}\"\n            >\n                <img\n                    src=\"{{getImageManagerImage theme_settings.background-image-snippet-7}}\"\n                    alt=\"{{theme_settings.btn-text-snippet-7}}\"\n                />\n            </a>\n        </div>\n    {{/if}}\n\n    {{#if theme_settings.background-image-snippet-8 \"!==\" \"\"}}\n        <div class=\"content-snippet-carousel__item content-snippet-carousel__item--8\">\n            <a\n                href=\"{{theme_settings.btn-url-snippet-8}}\"\n                title=\"{{theme_settings.btn-text-snippet-8}}\"\n            >\n                <img\n                    src=\"{{getImageManagerImage theme_settings.background-image-snippet-8}}\"\n                    alt=\"{{theme_settings.btn-text-snippet-8}}\"\n                />\n            </a>\n        </div>\n    {{/if}}\n\n    <button\n        aria-label=\"{{lang 'carousel.arrowAriaLabel'}}\"\n        class=\"js-hero-next-arrow slick-next slick-arrow\"\n    ></button>\n</div>\n{{else}}\n<div class=\"row no-gutters\">\n    <div class=\"col-12 col-md-6 col-lg-3\">\n        <div class=\"snippetContainer snippetContainer1\">\n            {{> components/custom/contentSnippet/home-content-snippet-1}}\n        </div>\n    </div>\n    <div class=\"col-12 col-md-6 col-lg-3\">\n        <div class=\"snippetContainer snippetContainer2\">\n            {{> components/custom/contentSnippet/home-content-snippet-2}}\n        </div>\n    </div>\n    <div class=\"col-12 col-md-6 col-lg-3\">\n        <div class=\"snippetContainer snippetContainer3\">\n            {{> components/custom/contentSnippet/home-content-snippet-3}}\n        </div>\n    </div>\n    <div class=\"col-12 col-md-6 col-lg-3\">\n        <div class=\"snippetContainer snippetContainer4\">\n            {{> components/custom/contentSnippet/home-content-snippet-4}}\n        </div>\n    </div>\n</div>\n{{/if}}\n",
  "components/custom/contentSnippet/home-content-snippet-1": "<div class=\"snippet-content {{#if theme_settings.background-image-snippet-1 \"==\" \"\"}}no-snippet-img{{else}}{{#if theme_settings.image-flip-snippet-1 '==' 'none'}}snippet-flip-n {{/if}}{{#if theme_settings.image-flip-snippet-1 '==' 'horizontal'}}snippet-flip-h{{/if}}{{#if theme_settings.image-flip-snippet-1 '==' 'vertical'}}snippet-flip-v{{/if}}{{/if}}\">\n\t{{#if theme_settings.background-image-snippet-1 \"!==\" \"\"}}\n\t\t<div class=\"snippet-content-img\" style=\"background-image: url('{{getImageManagerImage theme_settings.background-image-snippet-1}}');\">\n\t\t</div>\n\t{{/if}}\n\t<div class=\"snippet-content-figure\">\n\t\t<div class=\"snippet-content-wrap {{#if theme_settings.font-icon-snippet-1 \"==\" \"\"}}snippet-icon-none{{else}}{{#if theme_settings.icon-position-snippet-1 \"==\" \"top\"}}snippet-icon-top{{/if}}{{#if theme_settings.icon-position-snippet-1 \"==\" \"right\"}}snippet-icon-right{{/if}}{{#if theme_settings.icon-position-snippet-1 \"==\" \"bottom\"}}snippet-icon-bottom{{/if}}{{#if theme_settings.icon-position-snippet-1 \"==\" \"left\"}}snippet-icon-left{{/if}}{{/if}}\">\n\t\t\t{{#if theme_settings.icon-position-snippet-1 \"!==\" \"bottom\"}}\n\t\t\t\t<i class=\"{{theme_settings.font-icon-snippet-1}} {{theme_settings.icon-size-snippet-1}}\"></i>\n\t\t\t{{/if}}\n\t        <h4 class=\"snippet-title\">{{theme_settings.title-text-snippet-1}}</h4>\n\t        <p class=\"snippet-des\">{{theme_settings.body-text-snippet-1}}</p>\n\t        {{#if theme_settings.icon-position-snippet-1 \"==\" \"bottom\"}}\n\t\t\t\t<i class=\"{{theme_settings.font-icon-snippet-1}} {{theme_settings.icon-size-snippet-1}}\"></i>\n\t\t\t{{/if}}\n\t\t\t{{#if theme_settings.btn-text-snippet-1 \"!==\" \"\"}}\n\t\t\t\t<p class=\"snippet-btn\"><a href=\"{{theme_settings.btn-url-snippet-1}}\">{{theme_settings.btn-text-snippet-1}}</a></p>\n\t\t\t{{/if}}\n    \t</div>\n\t</div>\n</div>\n",
