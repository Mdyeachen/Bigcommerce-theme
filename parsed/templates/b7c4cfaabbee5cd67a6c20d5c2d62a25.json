{
  "ysw/addons/mega-nav/mega-nav-menu": "<div class=\"ysw-c-mega-nav navPages-container\" id=\"menu\">\r\n    <div class=\"ysw-c-mega-nav__wrapper\" style=\"background-color: #acacac;\">\r\n        <div class=\"navPages-quickSearch\">\r\n            {{#if theme_settings.epic_hide_search_navigation}}\r\n                {{> components/common/quick-search data='false'}}\r\n            {{/if}}\r\n\r\n            <div class=\"mobile-call-us\">\r\n                <a href=\"tel:{{settings.phone_number}}\">Call Us</a>\r\n            </div>\r\n            {{#if customer}}\r\n            <div class=\"mobile-my-account\">\r\n                <a href=\"{{urls.account.index}}\">My Account</a>\r\n            </div>\r\n            <div class=\"mobile-my-account\">\r\n                <a href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n            </div>\r\n            {{else}}\r\n                <div class=\"mobile-my-account\">\r\n                    <a href=\"{{urls.auth.login}}\">Sign in / Register</a>\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n        <nav class=\"ysw-c-mega-nav__navigation navPages\">\r\n            {{#if theme_settings.navigation_design '===' 'custom'}}\r\n                {{>\r\n                    ysw/addons/mega-nav/mega-nav-button\r\n                    position=\"left\"\r\n                    icon=\"chevron-down\"\r\n                }}\r\n            {{/if}}\r\n\r\n            <ul class=\"ysw-nav-item-last ysw-c-mega-nav__list navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n                {{#if theme_settings.navigation_design '===' 'custom'}}\r\n                    {{#for 0 (subtract theme_settings.ysw_meganav_item_qty 1)}}\r\n                        {{> ysw/addons/mega-nav/mega-nav-item theme_settings=../theme_settings}}\r\n                    {{/for}}\r\n                {{/if}}\r\n            </ul>\r\n            \r\n            <style>\r\n                \r\n                .ysw-c-mega-nav__item.navPages-item:last-child {\r\n                \theight: 0;\r\n                \twidth: 0;\r\n                \tvisibility: hidden;\r\n                \topacity: 0;\r\n                }\r\n                /*Min layout=800px  */\r\n                @media (min-width: 801px) {\r\n                  .navPage-subMenu {\r\n                    width: auto;\r\n                    left: 50% !important;;\r\n                    transform: translateX(-50%);\r\n                    }\r\n                }\r\n                    \r\n                /*Min layout=800px  */\r\n                @media (max-width:800px) {\r\n                     ul.ysw-nav-item-last {\r\n                    \tmargin-bottom: 300px !important;\r\n                    }\r\n                }\r\n            </style>\r\n            \r\n            {{#if theme_settings.navigation_design '===' 'custom'}}\r\n                {{>\r\n                    ysw/addons/mega-nav/mega-nav-button\r\n                    position=\"right\"\r\n                    icon=\"chevron-down\"\r\n                }}\r\n            {{/if}}\r\n        </nav>\r\n    </div>\r\n</div>\r\n",
  "components/common/quick-search": "<div class=\"container\">\r\n    <!-- <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form> -->\r\n    <form class=\"form\" action=\"{{urls.search}}\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\"\r\n                       {{#if data '===' 'false'}}{{else}}data-search-quick{{/if}}\r\n                       name=\"{{name}}\"\r\n                       id=\"{{name}}\"\r\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\r\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\r\n                       autocomplete=\"off\"\r\n                >\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n</div>\r\n",
  "ysw/addons/mega-nav/mega-nav-button": "<button\n    type=\"button\"\n    class=\"ysw-c-mega-nav__button ysw-c-mega-nav__button--{{position}}\"\n    aria-label=\"{{lang 'ysw.component.mega_scroll_menu.button' position=position}}\"\n    hidden\n>\n    <span class=\"ysw-c-mega-nav__button-icon\">\n        <i class=\"ysw-c-mega-nav__button-svg icon\" aria-hidden=\"true\">\n            <svg>\n                <use xlink:href=\"#icon-{{icon}}\"></use>\n            </svg>\n        </i>\n    </span>\n</button>\n",
  "ysw/addons/mega-nav/mega-nav-item": "<li class=\"ysw-c-mega-nav__item navPages-item\"\n    {{#if (get (concat 'ysw_meganav_item_children_' (sum $index 1)) theme_settings)}}\n        data-ysw-dropdown=\"navPages-{{$index}}\"\n        tabindex=\"0\"\n    {{/if}}\n    >\n    {{> ysw/addons/mega-nav/mega-nav-list\n        id=$index\n        name=(get (concat 'ysw_meganav_item_name_' (sum $index 1)) theme_settings)\n        url=(get (concat 'ysw_meganav_item_url_' (sum $index 1)) theme_settings)\n        children=(get (concat 'ysw_meganav_item_children_' (sum $index 1)) theme_settings)\n    }}\n</li>\n",
  "ysw/addons/mega-nav/mega-nav-list": "{{#if children}}\n    <a class=\"ysw-c-mega-nav__item-action navPages-action has-subMenu {{#if is_active}}activePage{{/if}}\"\n        href=\"{{url}}\"\n    >\n        {{name}}\n        <i class=\"ysw-c-mega-nav__item-icon icon navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg>\n                <use xlink:href=\"#icon-chevron-down\" />\n            </svg>\n        </i>\n    </a>\n\n    <div class=\"ysw-c-mega-nav__sub-menu navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n        {{{region name=(concat (concat \"navigation-menu-\" id) \"--global\")}}}\n    </div>\n{{else}}\n        <a class=\"ysw-c-mega-nav__item-action navPages-action{{#if is_active}} activePage{{/if}}\"\n            href=\"{{url}}\"\n            aria-label=\"{{name}}\"\n        >\n            {{name}}\n        </a>\n{{/if}}\n"
}
